liblanguagetool.la.closure: $(liblanguagetool_la_OBJECTS) $(liblanguagetool_la_DEPENDENCIES)
	@echo "int main() {return 0;}" > liblanguagetool_la_closure.cc
	@$(LTCXXCOMPILE) -c liblanguagetool_la_closure.cc
	$(CXXLINK) liblanguagetool_la_closure.lo $(liblanguagetool_la_LDFLAGS) $(liblanguagetool_la_OBJECTS) $(liblanguagetool_la_LIBADD) $(LIBS)
	@rm -f liblanguagetool_la_closure.* liblanguagetool.la.closure
	@echo "timestamp" > liblanguagetool.la.closure


