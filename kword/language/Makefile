bcheck: bcheck-am

bcheck-am:
	for i in $(include_HEADERS); do \
	    echo "int main() {return 0;}" > bchecktest.cc; \
	    echo "#include \"$$i\"" >> bchecktest.cc; \
	    $(CXXCOMPILE) --dump-class-hierarchy bchecktest.cc; \
	done
	-rm -f bchecktest.cc


#>+ 11
liblanguagetool_la.all_cc.cc: $(srcdir)/Makefile.in $(srcdir)/main.cc $(srcdir)/ConfigDialog.cc  ConfigDialog.moc main.moc
	@echo 'creating liblanguagetool_la.all_cc.cc ...'; \
	rm -f liblanguagetool_la.all_cc.files liblanguagetool_la.all_cc.final; \
	echo "#define KDE_USE_FINAL 1" >> liblanguagetool_la.all_cc.final; \
	for file in main.cc ConfigDialog.cc ; do \
	  echo "#include \"$$file\"" >> liblanguagetool_la.all_cc.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> liblanguagetool_la.all_cc.final; \
	done; \
	cat liblanguagetool_la.all_cc.final liblanguagetool_la.all_cc.files > liblanguagetool_la.all_cc.cc; \
	rm -f liblanguagetool_la.all_cc.final liblanguagetool_la.all_cc.files

