<rules>
	<rulegroup id="vesszo" name="vesszo szabalyok">
	<message>vesszo szabalyok</message>
	<rule id="bar" name="Vesszo ige utan bar,noha elott">
		<pattern lang="hu">(V1|V2|V3|V4|V5|V6|IKV1|IKV2|IKV3|IKV4|IKV5|IKV6|SI1|SI2|SI3|SI4|SI5|SI6|IKSI1|IKSI2|IKSI3|IKSI4|IKSI5|IKSI6) RART ^"(hogy|sõt)"</pattern>
		<message>Vesszo ige utan bar,noha elott</message>
		<example type="correct">kiment<em>,</em> pedig</example>
		<example type="incorrect">kiment pedig</example>
	</rule>
	<rule id="hogy" name="Vesszo ige utan hogy elott">
		<pattern lang="hu">(V1|V2|V3|V4|V5|V6|IKV1|IKV2|IKV3|IKV4|IKV5|IKV6|SI1|SI2|SI3|SI4|SI5|SI6|IKSI1|IKSI2|IKSI3|IKSI4|IKSI5|IKSI6) "(hogy|mert|sõt|viszont|nemhogy|mégis)"</pattern>
		<message>Vesszo ige utan bar,noha, megis elott</message>
		<example type="correct">kiment<em>,</em> hogy</example>
		<example type="incorrect">kiment hogy</example>
	</rule>
	</rulegroup>
	<rulegroup id="igekoto" name="Igekotos segedige">
	<message>meg tud irni</message>
	<rule id="IKSI" name="Igekotos segedige">
		<pattern lang="hu">(IKSI1|IKSI2|IKSI3|IKSI4|IKSI5|IKSI6) INF</pattern>
		<message>igekotos segedige</message>
		<example type="correct">meg tud irni</example>
		<example type="incorrect">megtud irni</example>
	</rule>
	</rulegroup>
	<rulegroup id="Ige-fonev egyeztetes" name="ige, nevszo egyeztetes">
	<rule id="NSV" name="Egyesszamu fonev utan ige egyesszam">
		<pattern lang="hu">NSN (V4|V5|V6|IKV4|IKV5|IKV6|SI4|SI5|SI6|IKSI4|IKSI5|IKSI6)</pattern>
		<message>Egyesszamu fonev utan ige egyesszam</message>
		<example type="correct">eger tancol</example>
		<example type="incorrect">eger tancolnak</example>
	</rule>
	<rule id="NPV" name="Tobbesszamu fonev utan ige tobbesszam">
		<pattern lang="hu">NPN (V1|V2|V3|IKV1|IKV2|IKV3|SI1|SI2|SI3|IKSI1|IKSI2|IKSI3)</pattern>
		<message>Tobbesszamu fonev utan ige tobbesszam</message>
		<example type="correct">egerek tancolnak</example>
		<example type="incorrect">egerek tancol</example>
	</rule>
	</rulegroup>
	<rulegroup id="Igek" name="Igek es szemelyes nevmas">
	<message>En utan egyes szam elso szemely</message>
	<rule id="EN" name="En utan egyes szam elso szemely">
		<pattern lang="hu">PP1 (V2|V3|V4|V5|V6|IKV2|IKV3|IKV4|IKV5|IKV6|SI2|SI3|SI4|SI5|SI6|IKSI2|IKSI3|IKSI4|IKSI5|IKSI6)</pattern>
		<message>En utan egyes szam elso szemely</message>
		<example type="correct">En megyek</example>
		<example type="incorrect">En megy</example>
	</rule>
	<rule id="TE" name="Te utan egyes szam masodik szemely">
		<pattern lang="hu">PP2 (V1|V3|V4|V5|V6|IKV1|IKV3|IKV4|IKV5|IKV6|SI1|SI3|SI4|SI5|SI6|IKSI1|IKSI3|IKSI4|IKSI5|IKSI6)</pattern>
		<message>Te utan egyes szam masodik szemely</message>
		<example type="correct">Te mesz</example>
		<example type="incorrect">Te megy</example>
	</rule>
	<rule id="O" name="O utan egyes szam harmadik szemely">
		<pattern lang="hu">PP3 (V1|V2|V4|V5|V6|IKV1|IKV2|IKV4|IKV5|IKV6|SI1|SI2|SI4|SI5|SI6|IKSI1|IKSI2|IKSI4|IKSI5|IKSI6)</pattern>
		<message>O utan egyes szam harmadik szemely</message>
		<example type="correct">o megy</example>
		<example type="incorrect">o mesz</example>
	</rule>
	<rule id="TI" name="Ti utan tobbes szam masodik szemely">
		<pattern lang="hu">PP5 (V1|V2|V3|V4|V6|IKV1|IKV2|IKV3|IKV4|IKV6|SI1|SI2|SI3|SI4|SI6|IKSI1|IKSI2|IKSI3|IKSI4|IKSI6)</pattern>
		<message>Ti utan tobbes szam masodik szemely</message>
		<example type="correct">ti mentek</example>
		<example type="incorrect">ti mesz</example>
	</rule>
	<rule id="OK" name="Ok utan tobbes szam harmadik szemely">
		<pattern lang="hu">PP6 (V1|V2|V3|V4|V5|IKV1|IKV2|IKV3|IKV4|IKV5|SI1|SI2|SI3|SI4|SI5|IKSI1|IKSI2|IKSI3|IKSI4|IKSI5)</pattern>
		<message>Ok utan tobbes szam harmadik szemely</message>
		<example type="correct">ok mennek</example>
		<example type="incorrect">ok mesz</example>
	</rule>
	<rule id="CRDNP" name="Szam utan egyesszamu fonev">
		<pattern lang="hu">CRD (NP|NPN)</pattern>
		<message>Szam utan egyesszamu fonev</message>
		<example type="correct">tiz cica</example>
		<example type="incorrect">tiz cicak</example>
	</rule>
	<rule id="ADJADV" name="Melleknev hatarozo helyett">
		<pattern lang="hu">ADJS (V1|V2|V3|V4|V5|V6|IKV1|IKV2|IKV3|IKV4|IKV5|IKV6)</pattern>
		<message>Melleknev hatarozo helyett</message>
		<example type="correct">szepen beszel</example>
		<example type="incorrect">szep beszel</example>
	</rule>
	<rule id="INFEN" name="En es fonevi igenev utan egyes szam elso szemelyu ige">
		<pattern lang="hu">PP1 VINF ^(V1|VT1|IKV1|IKVT1|SI1|SIT1|IKSI1|IKSIT1)</pattern>
		<message>En es fonevi igenev utan egyes szam elso szemelyu ige</message>
		<example type="correct">en menni fogok</example>
		<example type="incorrect">en menni fogsz</example>
	</rule>
	<rule id="INFTE" name="Te es fonevi igenev utan egyes szam masodik szemelyu ige">
		<pattern lang="hu">PP2 VINF ^(V2|VT2|IKV2|IKVT2|SI2|SIT2|IKSI2|IKSIT2)</pattern>
		<message>Te es fonevi igenev utan egyes szam masodik szemelyu ige</message>
		<example type="correct">te menni fogsz</example>
		<example type="incorrect">te menni fogok</example>
	</rule>
	<rule id="INFO" name="O es fonevi igenev utan egyes szam harmadik szemelyu ige">
		<pattern lang="hu">PP3 VINF ^(V3|VT3|IKV3|IKVT3|SI3|SIT3|IKSI3|IKSIT3)</pattern>
		<message>O es fonevi igenev utan egyes szam harmadik szemelyu ige</message>
		<example type="correct">o menni fog</example>
		<example type="incorrect">o menni fogok</example>
	</rule>
	<rule id="INFMI" name="Mi es fonevi igenev utan tobbes szam elso szemelyu ige">
		<pattern lang="hu">PP4 VINF ^(V4|VT4|IKV4|IKVT4|SI4|SIT4|IKSI4|IKSIT4)</pattern>
		<message>Mi es fonevi igenev utan tobbes szam elso szemelyu ige</message>
		<example type="correct">mi menni fogunk</example>
		<example type="incorrect">mi menni fogok</example>
	</rule>
	<rule id="INFTI" name="Ti es fonevi igenev utan tobbes szam masodik szemelyu ige">
		<pattern lang="hu">PP5 VINF ^(V5|VT5|IKV5|IKVT5|SI5|SIT5|IKSI5|IKSIT5)</pattern>
		<message>Ti es fonevi igenev utan tobbes szam masodik szemelyu ige</message>
		<example type="correct">ti menni fogtok</example>
		<example type="incorrect">ti menni fogok</example>
	</rule>
	<rule id="INFOK" name="Ok es fonevi igenev utan tobbes szam harmadik szemelyu ige">
		<pattern lang="hu">PP6 VINF ^(V6|VT6|IKV6|IKVT6|SI6|SIT6|IKSI6|IKSIT6)</pattern>
		<message>Ok es fonevi igenev utan tobbes szam harmadik szemelyu ige</message>
		<example type="correct">ok menni fognak</example>
		<example type="incorrect">ok menni fogtok</example>
	</rule>
	<rule id="KI" name="Ki utan tobbes szamu ige">
		<pattern lang="hu">"(kicsoda|micsoda)" (V4|V5|V6|IKV4|IKV5|IKV6) ^(VINF)</pattern>
		<message>Ki, mi utan tobbes szamu ige</message>
		<example type="correct">ki van ott</example>
		<example type="incorrect">ki vannak ott</example>
	</rule>
	<rule id="KI2" name="Ki utan tobbes szamu ige_2">
		<pattern lang="hu">"(ki)" (V4|V5|V6|IKV4|IKV5|IKV6) ^(VINF)</pattern>
		<message>Ki, mi utan tobbes szamu ige_2</message>
		<example type="correct">ki van ott</example>
		<example type="incorrect">ki vannak ott</example>
	</rule>
	<rule id="KIK" name="Kik utan egyes szamu ige">
		<pattern lang="hu">"(kik|kicsodák|mik|micsodák)" (V1|V2|V3|IKV1|IKV2|IKV3|SI1|SI2|SI3|IKSI1|IKSI2|IKSI3)</pattern>
		<message>Kik, mik utan egyes szamu ige</message>
		<example type="correct">ki van ott</example>
		<example type="incorrect">kik van ott</example>
	</rule>
	<rule id="MEGVAN" name="megvan egybe irando">
		<pattern lang="hu">"(M|m)eg" "van" "(a|az)"</pattern>
		<message>megvan egybe irando</message>
		<example type="correct">megvan a toll</example>
		<example type="incorrect">meg van a velemenyem</example>
	</rule>
	<rule id="NEIK" name="nem elvalo igekoto">
		<pattern lang="hu">NEIK (V1|V2|V3|V4|V5|V6)"</pattern>
		<message>at nem elvalo igekoto</message>
		<example type="correct">atert</example>
		<example type="incorrect">at ert</example>
	</rule>
	<rule id="HANY" name="Hany utan tobbes szamu fonev">
		<pattern lang="hu">INTRN (NP|NPN)</pattern>
		<message>Hany utan tobbes szamu fonev</message>
		<example type="correct">hany ember</example>
		<example type="incorrect">mennyi emberek</example>
	</rule>
	</rulegroup>
	<rulegroup id="melleknev" name="melleknev szama">
	<message>melleknev szama</message>
	<rule id="MNTOBB" name="Tobbesszamu mn, tobbesszamu fn">
		<pattern lang="hu">ADJP "(a|az)" NSN ^(ADV|NPR|NSR|N)</pattern>
		<message>Tobbesszamu mn, tobbesszamu fn</message>
		<example type="correct">szepek a lanyok</example>
		<example type="correct">szepek a lany fele menok</example>
		<example type="incorrect">szepek a lany</example>
	</rule>
	<rule id="FNTOBB" name="Tobbesszamu fn, tobbesszamu mn">
		<pattern lang="hu">NPN ADJS ^(N|NP|NS|NN|NPN|NSN)</pattern>
		<message>Tobbesszamu fn, tobbesszamu mn</message>
		<example type="correct">lanyok szepek</example>
		<example type="incorrect">lany szepek</example>
	</rule>
	<rule id="MNEGY" name="Egyesszamu mn, egyesszamu fn">
		<pattern lang="hu">ADJS "(a|az)" NPN</pattern>
		<message>Egyesszamu mn, egyesszamu fn</message>
		<example type="correct">szep a lany</example>
		<example type="incorrect">szep a lanyok</example>
	</rule>
	<rule id="FNEGY" name="Egyesszamu fn, egyesszamu mn">
		<pattern lang="hu">NSN ADJP ^(N|NP|NS|NN|NPN|NSN)</pattern>
		<message>Egyesszamu fn, egyesszamu mn</message>
		<example type="correct">lany szep</example>
		<example type="incorrect">lany szepek</example>
	</rule>
	<rule id="ADJN" name="Mennyisegjelzo utan egyes szamu fn">
		<pattern lang="hu">ADJN (NP|NPN)</pattern>
		<message>Mennyisegjelzo utan egyes szamu fn</message>
		<example type="correct">sok alma</example>
		<example type="incorrect">sok almák</example>
	</rule>
	<rule id="ADJN" name="Mennyisegjelzo utan egyes szamu mn">
		<pattern lang="hu">ADJN ADJP</pattern>
		<message>Mennyisegjelzo utan egyes szamu mn</message>
		<example type="correct">sok ugyes</example>
		<example type="incorrect">sok ugyesek</example>
	</rule>
	</rulegroup>
	<rulegroup id="egyedi" name="egyedi szavak v. csoportok">
	<message>megyedi szavak</message>
	<rule id="mer1" name="mer miert helyett">
		<pattern lang="hu">"mér" (V1|V2|V3|IKV1|IKV2|IKV3|SI1|SI2|SI3|IKSI1|IKSI2|IKSI3|V4|V5|V6|IKV4|IKV5|IKV6|SI4|SI5|SI6|IKSI4|IKSI5|IKSI6)</pattern>
		<message>mer miert helyett</message>
		<example type="correct">mer a merovel</example>
		<example type="incorrect">mer nem jo</example>
	</rule>
	<rule id="mer2" name="mer miert helyett">
		<pattern lang="hu">"(M|m)ér" "(nem)"</pattern>
		<message>mer miert helyett</message>
		<example type="correct">mer a merovel</example>
		<example type="incorrect">mer nem jo</example>
	</rule>
	<rule id="kolcs" name="Kolcsonoseen, kolcsonosen keverese">
		<pattern lang="hu">"(k|K)ölcsönösén"</pattern>
		<message>Kolcsonoseen, kolcsonosen keverese</message>
	</rule>
	<rule id="mellet" name="Mellett, mellet keverese">
		<pattern lang="hu">"(M|m)(ellet)"</pattern>
		<message>Mellett, mellet keverese</message>
		<example type="correct">mellett</example>
		<example type="incorrect">mellet</example>
	</rule>
	<rule id="mellet" name="kellett, kellet keverese">
		<pattern lang="hu">"(K|k)(ellet)"</pattern>
		<message>Kellett, kellet keverese</message>
	</rule>
	<rule id="egyen" name="Egyenlore, egyelore  keverese">
		<pattern lang="hu">"(E|e)gyenlõre" ^(V1|V2|V3|IKV1|IKV2|IKV3|SI1|SI2|SI3|IKSI1|IKSI2|IKSI3|V4|V5|V6|IKV4|IKV5|IKV6|SI4|SI5|SI6|IKSI4|IKSI5|IKSI6)</pattern>
		<message>Egyenlore, egyelore  keverese</message>
	</rule>
	<rule id="legyen" name="nem legyen">
		<pattern lang="hu">"(N|n)em" "legyen"</pattern>
		<message>nem ne helyett</message>
		<example type="correct">ne legyen</example>
		<example type="incorrect">nem legyen</example>
	</rule>
	<rule id="legyen" name="aru aru keverese">
		<pattern lang="hu">"(á|Á)ru" "árú"</pattern>
		<message>aru es aru keverese</message>
		<example type="correct">aruu aru</example>
		<example type="incorrect">aru aruu</example>
	</rule>
	<rule id="fen" name="fen es fenn keverese">
		<pattern lang="hu">"(F|f)en" (V1|V2|V3|IKV1|IKV2|IKV3|SI1|SI2|SI3|IKSI1|IKSI2|IKSI3|V4|V5|V6|IKV4|IKV5|IKV6|SI4|SI5|SI6|IKSI4|IKSI5|IKSI6)</pattern>
		<message>fen es fenn keverese</message>
		<example type="correct">fen kest</example>
		<example type="incorrect">fen megy</example>
	</rule>
	<rule id="fen2" name="fen es fenn keverese">
		<pattern lang="hu"> (V1|V2|V3|IKV1|IKV2|IKV3|SI1|SI2|SI3|IKSI1|IKSI2|IKSI3|V4|V5|V6|IKV4|IKV5|IKV6|SI4|SI5|SI6|IKSI4|IKSI5|IKSI6) "(F|f)en"</pattern>
		<message>fen es fenn keverese</message>
		<example type="correct">kest fen</example>
		<example type="incorrect">megy fen</example>
	</rule>
	<rule id="alt" name="alt es allt keverese">
		<pattern lang="hu">"(á|Á)lt" (ADJ|ADJS|ADV)</pattern>
		<message>allt es allt keverese</message>
		<example type="correct">allt ott</example>
		<example type="incorrect">alt ott</example>
	</rule>
	<rule id="alt2" name="alt es allt keverese">
		<pattern lang="hu">(ADJ|ADJS|ADV) "(Á|á)lt"</pattern>
		<message>allt es allt keverese</message>
		<example type="correct">ott allt</example>
		<example type="incorrect">ott alt</example>
	</rule>
	<rule id="fegyverzet" name="fegyverzet es fegyverzett keverese">
		<pattern lang="hu">(ADV) "(F|f)egyverzet"</pattern>
		<message>fegyverzet es fegyverzett</message>
		<example type="correct">jol fegyverzett</example>
		<example type="incorrect">jol fegyverzet</example>
	</rule>
	<rule id="mindenek" name="mindenek es mindennek keverese">
		<pattern lang="hu">"(M|m)indenek" "ellenére"</pattern>
		<message>mindenek es mindennek</message>
	</rule>
	<rule id="orokke" name="orokke es orokke keverese">
		<pattern lang="hu">"(Õ|õ)rökké"</pattern>
		<message>orokke es orokke keverese</message>
	</rule>
	<rule id="mert" name="mert es miert keverese">
		<pattern lang="hu">"(m|M)ért" (V1|V2|V3|IKV1|IKV2|IKV3|SI1|SI2|SI3|IKSI1|IKSI2|IKSI3|V4|V5|V6|IKV4|IKV5|IKV6|SI4|SI5|SI6|IKSI4|IKSI5|IKSI6)</pattern>
		<message>mert es miert keverese</message>
		<example type="correct">hosszat mert</example>
		<example type="incorrect">mert kiabalsz</example>
	</rule>
	<rule id="aztan" name="at tan es aztan keverese">
		<pattern lang="hu">"(A|a)z" "tán"</pattern>
		<message>az tan es aztan keverese</message>
		<example type="correct">aztan megirta</example>
		<example type="incorrect">az tan megirta</example>
	</rule>
	<rule id="hirtelen" name="hirtelen es hirtelen keverese">
		<pattern lang="hu">"(H|h)írtelen"</pattern>
		<message>hirtelen es hirtelen keverese</message>
	</rule>
	<rule id="eppen" name="epen es eppen keverese">
		<pattern lang="hu">"(É|é)pen"</pattern>
		<message>epen es eppen keverese</message>
	</rule>
	<rule id="modem" name="modem es modern keverese">
		<pattern lang="hu">"(m|M)odem"</pattern>
		<message>modem es modern  keverese</message>
	</rule>
	<rule id="viasza" name="viasza es vissza keverese">
		<pattern lang="hu">"(V|v)iasza"</pattern>
		<message>viasza es vissza keverese</message>
	</rule>
	<rule id="szukseg" name="szuukseg es szukseg keverese">
		<pattern lang="hu">"szûkség"</pattern>
		<message>szuukseg es szukseg keverese</message>
	</rule>
	<rule id="ujja" name="ujjaa es uujjaa keverese">
		<pattern lang="hu">"(U|u)jjá"</pattern>
		<message>ujjaa es uujjaa keverese</message>
	</rule>
	<rule id="kurt" name="kuurt es kurt keverese">
		<pattern lang="hu">"(K|k)ûrt"</pattern>
		<message>kuurt es kurt keverese</message>
	</rule>
	<rule id="torok" name="toorok es torok keverese">
		<pattern lang="hu">"(T|t)õrök"</pattern>
		<message>torok es toorok keverese</message>
	</rule>
	<rule id="irt" name="irt es iirt keverese">
		<pattern lang="hu">"(I|i)rt"</pattern>
		<message>irt es iirt keverese</message>
	</rule>
	<rule id="megvolt" name="meg volt">
		<pattern lang="hu">"(.*nak|.*nek)" "(meg|el|oda|ki)" "(volt.*|lesz.*|van.*)" "(a|az|egy)"</pattern>
		<message>meg volt egybeirando nak-nek utan</message>
	</rule>
	<rule id="megvolt2" name="meg volt">
		<pattern lang="hu">"(.*nak|.*nek)" CNJ "(meg|el|oda|ki)" "(volt.*|lesz.*|van.*)" "(a|az|egy)"</pattern>
		<message>meg volt egybeirando nak-nek utan</message>
	</rule>
	<rule id="megvolt3" name="megvolt">
		<pattern lang="hu">"((meg|le|oda|ki|el)(volt.*|van.*|lesz.*))" "(.*va|.*ve)"</pattern>
		<message>megvolt kulonirando va-ve elott</message>
	</rule>
	</rulegroup>
	<rule id="megtud" name="megtud, megmer">
		<pattern lang="hu">"((meg|le|be|meg|ki|bele|át|össze|szét|fel|föl|ide|oda|vissza|el|haza)(tud.*|mer.*|szeret.*|akar.*|kíván.*|óhajt.*))" "(.*ni)"</pattern>
		<message>megtud -mer kulonirando fonevi igenev elott</message>
	</rule>
	<rule id="szemel" name="szemel, szemmel">
		<pattern lang="hu">"(S|s)zemel"</pattern>
		<message>szemel, szemmel keverese</message>
	</rule>
	<rule id="ful" name="ful, fuul">
		<pattern lang="hu">"((F|f)ûl.*)"</pattern>
		<message>ful, fuul keverese</message>
	</rule>
	<rule id="siketul" name="sikerul, siketul">
		<pattern lang="hu">"((S|s)iketül.*)"</pattern>
		<message>siketul, sikerul keverese</message>
	</rule>
	<rule id="jelentoseg" name="jelentoseg es jelentosseg keverese">
